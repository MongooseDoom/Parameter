{
  "name": "Parameters",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extention adds/removes a parameter from a URL.",
  "homepage_url": "http://extensionizr.com",
  "options_page": "src/options/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Add/Remove Parameter"
  },
  "permissions": [
    "tabs"
  ]
}